{
	"name": "oldschooljs",
	"version": "0.1.3",
	"description": "Utility, API Wrapper library for Old School RuneScape (OSRS)",
	"main": "dist/index.js",
	"types": "dist/meta/index.d.ts",
	"scripts": {
		"prepublishOnly": "npm run prepare && npm run build",
		"test": "npm run build && npm run test:build && npm run test:run",
		"test:run": "node test/suite",
		"test:lint": "eslint --ext ts src",
		"test:build": "tsc -p test",
		"lint": "eslint --ext ts src scripts --fix",
		"build": "tsc -p .",
		"dev": "npm run prepare && tsc --watch -p .",
		"prepare": "npm run build && npm run prepare:build && npm run prepare:run",
		"prepare:build": "tsc -p scripts",
		"prepare:run": "node scripts/prepare"
	},
	"dependencies": {
		"jsdom": "^15.1.1",
		"node-fetch": "^2.6.0"
	},
	"devDependencies": {
		"@types/jsdom": "^12.2.4",
		"@types/node": "^12.6.2",
		"@types/node-fetch": "^2.3.7",
		"@types/tape": "^4.2.33",
		"@typescript-eslint/eslint-plugin": "^1.11.0",
		"@typescript-eslint/parser": "^1.11.0",
		"eslint": "^5.16.0",
		"eslint-config-bamboo": "^1.3.0",
		"eslint-config-prettier": "^4.3.0",
		"eslint-plugin-prettier": "^3.1.0",
		"husky": "^2.7.0",
		"prettier": "^1.18.2",
		"pretty-quick": "^1.11.1",
		"tape": "^4.11.0",
		"terser": "^4.1.2",
		"typescript": "^3.5.3"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pretty-quick --staged"
		}
	},
	"keywords": [
		"osrs",
		"runescape",
		"oldschool",
		"oldschoolrs"
	]
}
